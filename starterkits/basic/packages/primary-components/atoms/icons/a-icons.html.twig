<div class="a-icons">
  {% for filename in filenames %}
    <div class="a-icons__item">
      <div class="a-icons__name">{{ filename }}</div>
      <svg aria-hidden="true" class="a-icons__icon">
        <use xlink:href="{{ sprite }}#svg-{{ filename }}"></use>
      </svg>
    </div>
  {% endfor %}
</div>
